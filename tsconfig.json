{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "./dist/",
    "paths": {
      "react": ["node_modules/@types/react"],
      "tsnejs": ["node_modules/tsnejs/tsne.js"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "types", "configs/**/*"]
}
