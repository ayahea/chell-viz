{
  "name": "chell-viz",
  "version": "0.0.1",
  "author": "Biostatistics and Computational Biology at Dana-Farber <bcb@jimmy.harvard.edu>",
  "license": "MIT",
  "engines": {
    "node": "^9.6.0",
    "npm": "^5.6.0",
    "yarn": "^1.5.0"
  },
  "homepage": "https://cbiocenter.github.io/chell-viz/",
  "jest": {
    "collectCoverageFrom": ["src/**/*.{ts,tsx}"],
    "coverageDirectory": "dist/jest/coverage",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleDirectories": ["node_modules", "src", "types"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "moduleNameMapper": {
      "tsnejs": "tsnejs/tsne",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.ts"
    },
    "modulePathIgnorePatterns": ["dist"],
    "setupFiles": ["./configs/SetupJest.ts"],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "testEnvironment": "enzyme",
    "testMatch": ["**/__tests__/*.+(ts|tsx|js)"],
    "testPathIgnorePatterns": ["dist", "node_modules"],
    "testResultsProcessor": "jest-junit",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": ["<rootDir>/node_modules/(?!csstype)"],
    "verbose": true
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "classNameTemplate": "{classname}",
    "output": "dist/jest/test-results/junit.xml",
    "suiteName": "Chell-Viz Jest Tests",
    "titleTemplate": "{title}"
  },
  "lint-staged": {
    "*.{json,md,ts,tsx}": ["prettier --write", "git add"],
    "*.md": ["markdownlint -c ./markdownlint.json", "git add"],
    "*.{ts,tsx}": ["tslint", "git add"]
  },
  "scripts": {
    "build": "webpack --config ./configs/webpack.dev.ts",
    "build:prod": "webpack --config ./configs/webpack.prod.ts",
    "build:test": "tsc",
    "predeploy": "yarn build:prod && yarn doc && cp -r docs dist && cp README.md dist",
    "deploy": "gh-pages -d dist && git reset --hard HEAD",
    "doc":
      "rm -rf node_modules/typedoc/node_modules/typescript ; typedoc --out docs/api --includeDeclarations --excludeExternals --mode file",
    "doc:precommit": "yarn doc && git add docs/api && git add -u docs/api",
    "lint": "npm-run-all lint:*",
    "lint:src": "tslint --project ./",
    "lint:markdown": "markdownlint ./ -c ./markdownlint.json -i node_modules",
    "precommit": "lint-staged && yarn test:staged && yarn doc:precommit",
    "start": "webpack-dev-server --open --mode development --config ./configs/webpack.dev.ts",
    "start:hot": "webpack-dev-server --hot-only --open --mode development --config ./configs/webpack.dev.ts",
    "start:prod": "webpack-dev-server --open --mode production ./configs/webpack.prod.ts",
    "pretest": "yarn build:test && mkdir -p dist/jest/test-results",
    "test": "cross-env TEST_REPORT_PATH=dist/jest/test-results jest",
    "test:cover": "yarn test --collectCoverage true",
    "test:staged": "jest --onlyChanged"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-react": "^0.79.0"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/d3": "^5.0.0",
    "@types/enzyme": "^3.1.9",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/pixi.js": "^4.7.2",
    "@types/plotly.js": "^1.36.0",
    "@types/rc-slider": "^8.2.3",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "@types/recharts": "^1.0.18",
    "@types/signals": "^1.0.1",
    "@types/three": "^0.91.8",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.1.3",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "d3": "^5.0.0-rc.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^1.1.0",
    "global": "^4.3.2",
    "highcharts": "^6.1.0",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.3",
    "ify-loader": "^1.1.0",
    "jest": "^22.4.2",
    "jest-enzyme": "^5.0.3",
    "jest-fetch-mock": "^1.5.0",
    "jest-junit": "^3.6.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.7.0",
    "lint-staged": "^7.0.5",
    "markdownlint-cli": "^0.7.1",
    "ngl": "^2.0.0-dev.32",
    "npm-run-all": "^4.1.2",
    "pixi.js": "^4.7.1",
    "plotly.js": "^1.31.2",
    "prettier": "^1.11.1",
    "rc-slider": "^8.6.1",
    "react-test-renderer": "^16.2.0",
    "recharts": "^1.0.0-beta.10",
    "style-loader": "^0.20.3",
    "three": "^0.91.0",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.5.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  }
}
